{
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "import",
    "jest"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "no-console": "off",
  }
}
