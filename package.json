  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "dependencies": {
    "ascii-table": "0.0.9",
    "express": "^4.16.1",
    "mongodb": "^2.2.19",
    "mongoose": "^4.13.15",
    "mongoose-data-seed": "^1.0.6",
    "npm": "^6.4.0",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "cjson": "^0.5.0",
    "editorconfig": "^0.15.0",
    "eslint": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "http-server": "^0.10.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "jest-when": "^1.1.0",
    "nodemon": "^1.17.2",
    "regenerator-runtime": "^0.12.0",
    "rewire": "^2.5.2"
  },
  "scripts": {
    "test": "node node_modules/jest/bin/jest.js",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "coverage": "node node_modules/jest/bin/jest.js --coverage && google-chrome ./coverage/lcov-report/index.html",
    "start": "nodemon src/server.js --exec babel-node",
    "eslint": "node node_modules/eslint/bin/eslint.js -c .eslintrc.json src/**",
    "db": "mongod --dbpath c:/dev/mongo/data",
    "open": "concurrently \"mongod --dbpath c:/dev/mongo/data\" \"nodemon server.js\"",
    "seed": "md-seed run --dropdb",
    "clean": "rm -rf dist && mkdir dist",
    "build": "node node_modules/babel-cli/bin/babel.js src -d dist --source-maps",
    "serve": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:benprime/crucible-mud.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/benprime/crucible-mud#readme",
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testURL": "http://localhost",
    "roots": [
      "src/"
    ]
  }
